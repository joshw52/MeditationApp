	<div id='meditationTimer'>
		<table id='timerAdjust'>
			<tr><td><div id='hrup' onclick='modifyHour(1)'></div></td><td></td><td><div id='minup' onclick='modifyMinute(1)'></div></td><td></td><td><div id='secup' onclick='modifySecond(1)'></div></td></tr>
			<tr><td><span id='adjustHr' onclick='changeTimeVal("adjustHr", "hrInput")'>0</span><input id='hrInput' value='' style='display: none;'></td><td id='colonspacer'>:</td>
				<td><span id='adjustMin' onclick='changeTimeVal("adjustMin", "minInput")'>10</span><input id='minInput' value='' style='display: none;'></td><td id='colonspacer'>:</td>
				<td><span id='adjustSec' onclick='changeTimeVal("adjustSec", "secInput")'>00</span><input id='secInput' value='' style='display: none;'></td>
			</tr>
			<tr><td><div id='hrdown' onclick='modifyHour(-1)'></div></td><td></td><td><div id='mindown' onclick='modifyMinute(-1)'></div></td><td></td><td><div id='secdown' onclick='modifySecond(-1)'></div></td></tr>
		</table>

		<div id='timerButtons'>
			<button id='start' onclick='startTimer()'>Start</button>
			<button id='stop' onclick='stopTimer()'>Stop</button>
			<button id='reset' onclick='resetTimer()'>Reset</button>
		</div>

		<div id='buddhaFill'>
			<img id='buddhaFillImg' src='http://www.vagabondtemple.com/wp-content/uploads/2016/07/mantra-om-1292602.png'>
		</div>
	</div>

